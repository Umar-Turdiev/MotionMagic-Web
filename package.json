{
  "name": "motionmagic",
  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration local --disable-host-check",
    "build-dev": "ng build --configuration development --aot",
    "build-prod": "ng build --configuration production --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analyze-webpack": "ng build --configuration production --aot --stats-json && webpack-bundle-analyzer dist/motionmagic-release/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.3",
    "@angular/cdk": "^17.3.2",
    "@angular/common": "^18.0.3",
    "@angular/compiler": "^18.0.3",
    "@angular/core": "^18.0.3",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.0.3",
    "@angular/platform-browser": "^18.0.3",
    "@angular/platform-browser-dynamic": "^18.0.3",
    "@angular/router": "^18.0.3",
    "@dimforge/rapier2d": "^0.12.0",
    "@streamparser/json": "^0.0.21",
    "@types/fabric": "^5.3.7",
    "fabric": "^5.3.0",
    "firebase": "^10.12.2",
    "mathjax-angular": "^2.2.1",
    "nerdamer": "^1.1.13",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.4",
    "@angular/cli": "~18.0.4",
    "@angular/compiler-cli": "^18.0.3",
    "@types/hammerjs": "^2.0.45",
    "@types/jasmine": "~5.1.2",
    "@types/uuid": "^10.0.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
